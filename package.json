{
  "name": "vue-typeahead3",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-typeahead.umd.js",
  "module": "./dist/vue-typeahead.es.js",
  "style": "./dist/vue-typeahead.css",
  "exports": {
    ".": {
      "import": "./dist/vue-typeahead.es.js",
      "require": "./dist/vue-typeahead.umd.js"
    },
    "./dist/vue-typeahead.css": {
      "import": "./dist/vue-typeahead.css",
      "require": "./dist/vue-typeahead.css"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDeerly/vue-typeahead3.git"
  },
  "bugs": {
    "url": "https://github.com/MrDeerly/vue-typeahead3/issues"
  },
  "homepage": "https://github.com/MrDeerly/vue-typeahead3",
  "keywords": [
    "vue",
    "vue3",
    "search",
    "suggestion",
    "input",
    "list",
    "autocomplete",
    "autosuggest",
    "searchInput",
    "typeahead",
    "next",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.6.1",
    "@vue/compiler-sfc": "^3.2.6",
    "dart-sass": "^1.25.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^9.2.0",
    "prettier": "2.4.1",
    "sass": "^1.41.1",
    "typescript": "^4.4.3",
    "vite": "^2.5.4",
    "vite-plugin-eslint": "^1.3.0",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.29.5"
  },
  "description": "A super lightweight typeahead / autocompletion component for Vue.js 3. It's written in TypeScript using Vue.js 3 Composition API.",
  "author": "MrDeerly"
}
